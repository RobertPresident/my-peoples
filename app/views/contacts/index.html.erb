<h1>All Contacts</h1>



<div class="row">
  <% @contacts.each do |contact| %>
    <div class="col-md-4 col-sm-5">
      <div class="thumbnail list-view">
        <h2><a href="/contacts/<%= contact.id %>"><%= contact.first_name %></a></h2>
        <h3>First Name: <%= contact.first_name %></h3>
        <h3>Last Name: <%= contact.last_name %></h3>
        <h3>Email: <%= contact.email %></h3>
        <h3>Phone Number: <%= contact.phone_number %></h3>

        <br>
      </div>
    </div>
  <% end %>  
</div>













